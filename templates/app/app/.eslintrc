{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },

  "plugins": ["react"],

  "rules": {
    "no-tabs": 1,
    "no-unused-vars":0,
    "indent": [
      2, 2
    ]
  },

  "globals": {
    "page": true,
    "browser": true
  }
}