{
  "name": "takealot-admin-ui",
  "version": "0.0.4",
  "description": "General UI kit used for Takealot admin interfaces",
  "main": "index.js",
  "author": "www.takealot.com",
  "owner": "www.takealot.com",
  "license": "private",
  "scripts": {
    "start": "cd ../cli && npm i && npm start",
    "app-start": "BABEL_ENV=development NODE_ENV=development webpack-dev-server -d --no-info",
    "cli": "cd ../cli && node index.js",
    "build": "BABEL_ENV=production NODE_ENV=production webpack -p --config webpack.config.js",
    "build:analyze": "cross-env ANALYZE=true node scripts/build.js",
    "watch": "./node_modules/.bin/webpack -d --watch",
    "lint": "node_modules/.bin/eslint . --ext .js --ext .jsx",
    "test": "BABEL_ENV=TEST NODE_ENV=TEST jest --coverage",
    "stat": "webpack --profile --json > webpack-stats.json",
    "doc": "./node_modules/.bin/esdoc -c .esdoc.json",
    "clear": "rm -rf node_modules/.cache"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "autodll-webpack-plugin": "^0.3.9",
    "autoprefixer": "^8.0.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-react-autoprefix": "^1.0.1",
    "babel-polyfill": "^6.26.0",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.5",
    "es6-promise": "^4.2.2",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "find-in-files": "^0.5.0",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "jest-puppeteer-preset": "^2.3.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.5",
    "raven-js": "^3.24.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-raven": "^1.2.3",
    "react-router-dom": "^4.2.2",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-less": "^2.3.1",
    "semantic-ui-react": "^0.79.0",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-merge": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "esdoc": "^1.0.4",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-publish-markdown-plugin": "^1.0.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^22.4.3",
    "jest-environment-enzyme": "^6.0.0",
    "jest-enzyme": "^6.0.0",
    "jest-puppeteer": "^2.3.0",
    "jest-webpack-resolver": "^0.3.0",
    "puppeteer": "^1.3.0",
    "react-test-renderer": "^16.3.2",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-jarvis": "^0.3.0"
  }
